<script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCadduajTqkSICZsZ4B7ECHNRlqWOwddAw&callback=initialize"

  type="text/javascript"></script>

<script>
function initialize() {

  var wellLoc = {lat: 38.8072, lng: -122.8105};

  var mapProp = {
    center: new google.maps.LatLng(38.8072, -122.8105),
    zoom: 8,
    mapTypeId:google.maps.MapTypeId.HYBRID
  };

  var map = new google.maps.Map(document.getElementById("googleMap"), mapProp);


  var contentString = 'Happy Jack 12';

  var infowindow = new google.maps.InfoWindow({
    content: contentString
  });


  var marker = new google.maps.Marker({
    position: wellLoc,
    map: map,
    title: 'Hello World!'
  });

  marker.addListener('click', function() {
    infowindow.open(map, marker);
  });

}

google.maps.event.addDomListener(window, 'load', initialize);



</script>

<div id="googleMap">
	
</div>
