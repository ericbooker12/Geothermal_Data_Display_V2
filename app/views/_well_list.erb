	<% @wells.each do |well| %>
		<div id="well/<%= well.id%>">
			<h4><%= "#{well.name}" %></h4>
				<ul>
					<li><a href="/wells/<%= well.id %>">Data</a></li>
					<li><a href="/wells/<%= well.id %>">Charts</a></li>
				</ul>
		</div>
	<% end %>
	<span id="well_sidebar">	
	</span>

</div>
